package inherit_unnamed_detections
public
	system s1
		features
			f1: feature;
			f2: feature;
			f3: feature;
		annex EMV2 {**
			use behavior inherit_unnamed_detections::machine1;
			
			component error behavior
				detections
					state1 -[]-> f1 !;
			end component;
		**};
	end s1;
	
	system s2 extends s1
		annex EMV2 {**
			component error behavior
				detections
					state2 -[]-> f2 !;
			end component;
		**};
	end s2;
	
	system implementation s2.i
		annex EMV2 {**
			component error behavior
				detections
					state3 -[]-> f3 !;
			end component;
		**};
	end s2.i;
	
	annex EMV2 {**
		error behavior machine1
			states
				state1: initial state;
				state2: state;
				state3: state;
		end behavior;
	**};
end inherit_unnamed_detections;