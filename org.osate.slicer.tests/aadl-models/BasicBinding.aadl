	data d
		annex EMV2 {**
			use types ErrorLibrary;
			
			error propagations
				memory: out propagation {ServiceError};
				flows
					o2Src : error source memory {ServiceError};
			end propagations;
		**};
	end d;

	memory m
		annex EMV2 {**
			use types ErrorLibrary;
			
			error propagations
				bindings: in propagation {ServiceError};
				flows
					o2Snk : error sink bindings {ServiceError};
			end propagations;
		**};
	end m;

	bus b
		annex EMV2 {**
			use types ErrorLibrary;
			
			error propagations
				bindings: out propagation {ServiceError};
			end propagations;
		**};
	end b;

	virtual bus vb
		annex EMV2 {**
			use types ErrorLibrary;
			
			error propagations
				connection: in propagation {ServiceError};
			end propagations;
		**};
	end vb;
end BasicBinding;